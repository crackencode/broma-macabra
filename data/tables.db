mg":"modules/compendios-llc-es-basicos/icons/dado.svg","weight":1,"range":[10,10],"drawn":false,"rangeL":10,"rangeH":10}],"formula":"1d10","replacement":true,"displayRoll":true,"folder":"vKCx4PC5LUIf99y7","sort":100000,"permission":{"default":0,"DrIsbjsRd1cgvfpo":3,"ox85z1mKrAqFXzdN":3},"flags":{"core":{"sourceId":"Compendium.compendios-llc-es-basicos.tablas-aleatorias.ed1xMTLHzSKH1WSA"}}}
